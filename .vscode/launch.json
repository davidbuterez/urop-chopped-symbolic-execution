{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}/diffanalyze/",
      "args": [
        // "https://github.com/davidbuterez/password-example", "4d7add621bf6f54b520a02dd50c3aaf69a43b4f4"
        // "https://git.savannah.gnu.org/git/findutils.git", "c7344d33587bc5b781b958315c643284e2e9cf18" //, "--only-function-names"
        // "repo/password.c"
        // "https://github.com/davidbuterez/password-example", "65eea916396822dc4963c597063742b74e995ff9"
        // "https://git.savannah.gnu.org/git/findutils.git", "df4610d08d66e59c8fd0c8fcafcbb18dda1876cd"
        // "https://git.savannah.gnu.org/git/findutils.git", "22ff99e150faa82980361e577cff643cc17e7fac"
        // "https://git.savannah.gnu.org/git/findutils.git", "d2fee47412b18ef929649f0a9c7de99374fd20d4"
        // "https://github.com/davidbuterez/password-example", "4d7add621bf6f54b520a02dd50c3aaf69a43b4f4", "--cache", "--verbose"
        "https://git.savannah.gnu.org/git/findutils.git"
      ]
    },
    {
      "name": "Python: Attach",
      "type": "python",
      "request": "attach",
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "${workspaceFolder}",
      "port": 3000,
      "secret": "my_secret",
      "host": "localhost"
    },
    {
      "name": "Python: Terminal (integrated)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: Terminal (external)",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "externalTerminal"
    },
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "--noreload",
        "--nothreading"
      ],
      "debugOptions": [
        "RedirectOutput",
        "Django"
      ]
    },
    {
      "name": "Python: Flask (0.11.x or later)",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app.py"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ]
    },
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "module.name"
    },
    {
      "name": "Python: Pyramid",
      "type": "python",
      "request": "launch",
      "args": [
        "${workspaceFolder}/development.ini"
      ],
      "debugOptions": [
        "RedirectOutput",
        "Pyramid"
      ]
    },
    {
      "name": "Python: Watson",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/console.py",
      "args": [
        "dev",
        "runserver",
        "--noreload=True"
      ]
    },
    {
      "name": "Python: All debug Options",
      "type": "python",
      "request": "launch",
      "pythonPath": "${config:python.pythonPath}",
      "program": "${file}",
      "module": "module.name",
      "env": {
        "VAR1": "1",
        "VAR2": "2"
      },
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "arg1",
        "arg2"
      ],
      "debugOptions": [
        "RedirectOutput"
      ]
    }
  ]
}